<?xml version="1.0" encoding="UTF-8"?>
<lexicon version="1.0" language="en" xmlns="http://canopy.rs/lexicon">

  <metadata>
    <title>Canopy English Lexicon</title>
    <description>Comprehensive collection of closed-class words and functional lexical items for English NLP</description>
    <created>2024-01-15</created>
    <author>Canopy.rs Contributors</author>
    <license>MIT</license>
  </metadata>

  <word-classes>

    <!-- Stop Words / Function Words -->
    <word-class id="stop-words-basic" name="Basic Stop Words" type="stop-words" priority="10">
      <description>Core stop words that typically carry little semantic meaning</description>
      <properties>
        <property name="remove-in-indexing" value="true" type="boolean"/>
        <property name="semantic-weight" value="0.1" type="float"/>
      </properties>
      <words>
        <word pos="DT">the</word>
        <word pos="DT">a</word>
        <word pos="DT">an</word>
        <word pos="IN">of</word>
        <word pos="IN">in</word>
        <word pos="IN">on</word>
        <word pos="IN">at</word>
        <word pos="IN">by</word>
        <word pos="IN">for</word>
        <word pos="IN">with</word>
        <word pos="IN">to</word>
        <word pos="IN">from</word>
        <word pos="CC">and</word>
        <word pos="CC">or</word>
        <word pos="CC">but</word>
        <word pos="PRP">i</word>
        <word pos="PRP">you</word>
        <word pos="PRP">he</word>
        <word pos="PRP">she</word>
        <word pos="PRP">it</word>
        <word pos="PRP">we</word>
        <word pos="PRP">they</word>
        <word pos="VBZ">is</word>
        <word pos="VBP">are</word>
        <word pos="VBD">was</word>
        <word pos="VBD">were</word>
        <word pos="VB">be</word>
        <word pos="VBG">being</word>
        <word pos="VBN">been</word>
        <word pos="VBZ">has</word>
        <word pos="VBP">have</word>
        <word pos="VBD">had</word>
        <word pos="MD">will</word>
        <word pos="MD">would</word>
        <word pos="MD">could</word>
        <word pos="MD">should</word>
        <word pos="MD">can</word>
        <word pos="MD">may</word>
        <word pos="MD">might</word>
        <word pos="MD">must</word>
        <word pos="DT">this</word>
        <word pos="DT">that</word>
        <word pos="DT">these</word>
        <word pos="DT">those</word>
      </words>
    </word-class>

    <!-- Negation Words -->
    <word-class id="negation-words" name="Negation Words" type="negation" priority="9">
      <description>Words that indicate negation or denial</description>
      <properties>
        <property name="polarity" value="negative" type="string"/>
        <property name="scope-modifier" value="true" type="boolean"/>
      </properties>
      <words>
        <word pos="RB" confidence="1.0">not</word>
        <word pos="RB" variants="n't" confidence="1.0">not</word>
        <word pos="DT" confidence="1.0">no</word>
        <word pos="RB" confidence="1.0">never</word>
        <word pos="NN" confidence="0.9">nothing</word>
        <word pos="NN" confidence="0.9">nobody</word>
        <word pos="NN" confidence="0.9">none</word>
        <word pos="DT" confidence="0.9">neither</word>
        <word pos="RB" confidence="0.8">nowhere</word>
        <word pos="MD" confidence="0.9">cannot</word>
        <word pos="MD" variants="can't" confidence="0.9">cannot</word>
        <word pos="MD" variants="won't" confidence="0.9">will not</word>
        <word pos="MD" variants="wouldn't" confidence="0.9">would not</word>
        <word pos="MD" variants="couldn't" confidence="0.9">could not</word>
        <word pos="MD" variants="shouldn't" confidence="0.9">should not</word>
        <word pos="VBP" variants="don't" confidence="0.9">do not</word>
        <word pos="VBZ" variants="doesn't" confidence="0.9">does not</word>
        <word pos="VBD" variants="didn't" confidence="0.9">did not</word>
        <word pos="VBZ" variants="isn't" confidence="0.9">is not</word>
        <word pos="VBP" variants="aren't" confidence="0.9">are not</word>
        <word pos="VBD" variants="wasn't" confidence="0.9">was not</word>
        <word pos="VBD" variants="weren't" confidence="0.9">were not</word>
        <word pos="VBZ" variants="hasn't" confidence="0.9">has not</word>
        <word pos="VBP" variants="haven't" confidence="0.9">have not</word>
        <word pos="VBD" variants="hadn't" confidence="0.9">had not</word>
      </words>
      <patterns>
        <pattern id="neg-prefix-un" type="prefix" confidence="0.8">
          <regex>^un[a-z]+</regex>
          <description>Un- prefix indicating negation or reversal</description>
          <examples>
            <example>unhappy</example>
            <example>unable</example>
            <example>unsuccessful</example>
          </examples>
        </pattern>
        <pattern id="neg-prefix-dis" type="prefix" confidence="0.8">
          <regex>^dis[a-z]+</regex>
          <description>Dis- prefix indicating negation or reversal</description>
          <examples>
            <example>disagree</example>
            <example>disapprove</example>
            <example>disconnect</example>
          </examples>
        </pattern>
        <pattern id="neg-prefix-in" type="prefix" confidence="0.7">
          <regex>^in[a-z]+</regex>
          <description>In- prefix indicating negation (context dependent)</description>
          <examples>
            <example>incorrect</example>
            <example>incomplete</example>
            <example>inappropriate</example>
          </examples>
        </pattern>
        <pattern id="neg-prefix-im" type="prefix" confidence="0.8">
          <regex>^im[a-z]+</regex>
          <description>Im- prefix indicating negation (before p/b/m)</description>
          <examples>
            <example>impossible</example>
            <example>improbable</example>
            <example>immature</example>
          </examples>
        </pattern>
        <pattern id="neg-prefix-ir" type="prefix" confidence="0.9">
          <regex>^ir[a-z]+</regex>
          <description>Ir- prefix indicating negation (before r)</description>
          <examples>
            <example>irregular</example>
            <example>irresponsible</example>
            <example>irrelevant</example>
          </examples>
        </pattern>
        <pattern id="neg-prefix-il" type="prefix" confidence="0.9">
          <regex>^il[a-z]+</regex>
          <description>Il- prefix indicating negation (before l)</description>
          <examples>
            <example>illegal</example>
            <example>illogical</example>
            <example>illegible</example>
          </examples>
        </pattern>
        <pattern id="neg-prefix-non" type="prefix" confidence="0.9">
          <regex>^non[a-z]+</regex>
          <description>Non- prefix indicating absence or negation</description>
          <examples>
            <example>nonsense</example>
            <example>nonfiction</example>
            <example>nonviolent</example>
          </examples>
        </pattern>
        <pattern id="neg-suffix-less" type="suffix" confidence="0.8">
          <regex>[a-z]+less$</regex>
          <description>-less suffix indicating absence or lack</description>
          <examples>
            <example>hopeless</example>
            <example>careless</example>
            <example>homeless</example>
          </examples>
        </pattern>
      </patterns>
    </word-class>

    <!-- Discourse Markers -->
    <word-class id="discourse-markers" name="Discourse Markers" type="discourse-markers" priority="8">
      <description>Words and phrases that organize discourse and indicate relationships between ideas</description>
      <properties>
        <property name="discourse-function" value="true" type="boolean"/>
        <property name="text-structure" value="true" type="boolean"/>
      </properties>
      <words>
        <!-- Temporal/Sequential -->
        <word context="temporal">first</word>
        <word context="temporal">second</word>
        <word context="temporal">third</word>
        <word context="temporal">then</word>
        <word context="temporal">next</word>
        <word context="temporal">finally</word>
        <word context="temporal">meanwhile</word>
        <word context="temporal">subsequently</word>
        <word context="temporal">previously</word>
        <word context="temporal">earlier</word>
        <word context="temporal">later</word>
        <word context="temporal">afterwards</word>
        <word context="temporal">beforehand</word>

        <!-- Causal -->
        <word context="causal">therefore</word>
        <word context="causal">thus</word>
        <word context="causal">consequently</word>
        <word context="causal">hence</word>
        <word context="causal">accordingly</word>
        <word context="causal">because</word>
        <word context="causal">since</word>
        <word context="causal">so</word>

        <!-- Contrastive -->
        <word context="contrastive">however</word>
        <word context="contrastive">nevertheless</word>
        <word context="contrastive">nonetheless</word>
        <word context="contrastive">although</word>
        <word context="contrastive">though</word>
        <word context="contrastive">whereas</word>
        <word context="contrastive">while</word>
        <word context="contrastive">conversely</word>
        <word context="contrastive">instead</word>
        <word context="contrastive">rather</word>

        <!-- Additive -->
        <word context="additive">also</word>
        <word context="additive">additionally</word>
        <word context="additive">furthermore</word>
        <word context="additive">moreover</word>
        <word context="additive">besides</word>
        <word context="additive">likewise</word>
        <word context="additive">similarly</word>

        <!-- Conclusive -->
        <word context="conclusive">overall</word>
        <word context="conclusive">ultimately</word>
        <word context="conclusive">essentially</word>
        <word context="conclusive">basically</word>
        <word context="conclusive">fundamentally</word>
      </words>
      <patterns>
        <pattern id="discourse-phrase-on-hand" type="phrase" confidence="0.9">
          <regex>\bon the other hand\b</regex>
          <description>Contrastive discourse marker phrase</description>
          <examples>
            <example>on the other hand</example>
          </examples>
        </pattern>
        <pattern id="discourse-phrase-as-result" type="phrase" confidence="0.9">
          <regex>\bas a result\b</regex>
          <description>Causal discourse marker phrase</description>
          <examples>
            <example>as a result</example>
          </examples>
        </pattern>
        <pattern id="discourse-phrase-in-contrast" type="phrase" confidence="0.9">
          <regex>\bin contrast\b</regex>
          <description>Contrastive discourse marker phrase</description>
          <examples>
            <example>in contrast</example>
          </examples>
        </pattern>
      </patterns>
    </word-class>

    <!-- Quantifiers -->
    <word-class id="quantifiers" name="Quantifiers" type="quantifiers" priority="7">
      <description>Words indicating quantity, amount, or degree</description>
      <properties>
        <property name="quantification" value="true" type="boolean"/>
        <property name="scope-sensitive" value="true" type="boolean"/>
      </properties>
      <words>
        <!-- Universal -->
        <word context="universal">all</word>
        <word context="universal">every</word>
        <word context="universal">each</word>
        <word context="universal">both</word>
        <word context="universal">either</word>

        <!-- Existential -->
        <word context="existential">some</word>
        <word context="existential">any</word>
        <word context="existential">several</word>
        <word context="existential">many</word>
        <word context="existential">few</word>
        <word context="existential">most</word>
        <word context="existential">much</word>
        <word context="existential">little</word>

        <!-- Numerical -->
        <word context="numerical">one</word>
        <word context="numerical">two</word>
        <word context="numerical">three</word>
        <word context="numerical">four</word>
        <word context="numerical">five</word>
        <word context="numerical">six</word>
        <word context="numerical">seven</word>
        <word context="numerical">eight</word>
        <word context="numerical">nine</word>
        <word context="numerical">ten</word>
        <word context="numerical">hundred</word>
        <word context="numerical">thousand</word>
        <word context="numerical">million</word>
        <word context="numerical">billion</word>

        <!-- Ordinal -->
        <word context="ordinal">first</word>
        <word context="ordinal">second</word>
        <word context="ordinal">third</word>
        <word context="ordinal">fourth</word>
        <word context="ordinal">fifth</word>
        <word context="ordinal">last</word>

        <!-- Degree -->
        <word context="degree">very</word>
        <word context="degree">quite</word>
        <word context="degree">rather</word>
        <word context="degree">extremely</word>
        <word context="degree">highly</word>
        <word context="degree">completely</word>
        <word context="degree">absolutely</word>
        <word context="degree">totally</word>
        <word context="degree">entirely</word>
        <word context="degree">partially</word>
        <word context="degree">somewhat</word>
        <word context="degree">slightly</word>
        <word context="degree">barely</word>
        <word context="degree">hardly</word>
      </words>
      <patterns>
        <pattern id="number-pattern" type="whole-word" confidence="0.9">
          <regex>\b\d+\b</regex>
          <description>Numeric digits representing quantities</description>
          <examples>
            <example>5</example>
            <example>42</example>
            <example>1000</example>
          </examples>
        </pattern>
        <pattern id="ordinal-suffix" type="suffix" confidence="0.8">
          <regex>\d+(st|nd|rd|th)\b</regex>
          <description>Ordinal number suffixes</description>
          <examples>
            <example>1st</example>
            <example>2nd</example>
            <example>3rd</example>
            <example>4th</example>
          </examples>
        </pattern>
      </patterns>
    </word-class>

    <!-- Temporal Expressions -->
    <word-class id="temporal-expressions" name="Temporal Expressions" type="temporal" priority="6">
