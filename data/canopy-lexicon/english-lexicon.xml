      <description>Words indicating uncertainty, approximation, or hedging</description>
      <properties>
        <property name="certainty-modifier" value="true" type="boolean"/>
        <property name="epistemic" value="true" type="boolean"/>
      </properties>
      <words>
        <word>perhaps</word>
        <word>maybe</word>
        <word>possibly</word>
        <word>probably</word>
        <word>likely</word>
        <word>unlikely</word>
        <word>apparently</word>
        <word>seemingly</word>
        <word>allegedly</word>
        <word>supposedly</word>
        <word>presumably</word>
        <word>arguably</word>
        <word>potentially</word>
        <word>conceivably</word>
