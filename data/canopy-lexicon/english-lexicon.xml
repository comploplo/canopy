  <!-- Global Patterns -->
  <global-patterns>

    <!-- Contraction Patterns -->
    <pattern id="contractions-general" type="contraction" confidence="0.9">
      <regex>\b\w+'(s|re|ve|ll|d|t|m)\b</regex>
      <description>General contraction patterns</description>
      <examples>
        <example>he's</example>
        <example>they're</example>
        <example>I've</example>
        <example>we'll</example>
        <example>I'd</example>
        <example>can't</example>
        <example>I'm</example>
      </examples>
    </pattern>

    <!-- Possessive Patterns -->
    <pattern id="possessive-s" type="possessive" confidence="0.95">
      <regex>\b\w+'s\b</regex>
      <description>Possessive 's marker</description>
      <examples>
        <example>John's</example>
        <example>cat's</example>
        <example>children's</example>
      </examples>
    </pattern>

    <!-- Plural Possessive -->
    <pattern id="possessive-plural" type="possessive" confidence="0.9">
      <regex>\b\w+s'\b</regex>
      <description>Plural possessive marker</description>
      <examples>
        <example>cats'</example>
        <example>students'</example>
        <example>girls'</example>
      </examples>
    </pattern>

  </global-patterns>
