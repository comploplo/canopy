        <property name="epistemic-deontic" value="true" type="boolean"/>
      </properties>
      <words>
        <!-- Core modals --><word pos="MD" context="possibility">can</word>
        <word pos="MD" context="possibility">could</word>
        <word pos="MD" context="possibility">may</word>
        <word pos="MD" context="possibility">might</word>
        <word pos="MD" context="necessity">must</word>
