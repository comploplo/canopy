    <!-- Conjunctions (Extended) --><word-class id="conjunctions" name="Conjunctions" type="conjunctions" priority="8">
      <description>Coordinating and subordinating conjunctions</description>
      <properties>
        <property name="connective" value="true" type="boolean"/>
        <property name="clause-linking" value="true" type="boolean"/>
